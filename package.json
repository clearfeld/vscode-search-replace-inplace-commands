{
  "name": "search-replace-inplace-commands",
  "description": "Additional search replace and inplace commands for vscode.",
  "version": "0.0.1",
  "publisher": "clearfeld",
  "icon": "assets/icon.png",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clearfeld/vscode-search-replace-inplace-commands"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "extensionKind": [],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:clearfeld.minibufferViewConsultLine",
    "onCommand:clearfeld.consultLine"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "clearfeld-minibuffer-view-consult-line",
          "title": "Minibuffer",
          "icon": "fancy.icon",
          "when": "clearfeld.consultLine"
        }
      ]
    },
    "views": {
      "clearfeld-minibuffer-view-consult-line": [
        {
          "type": "webview",
          "id": "clearfeld.minibufferViewConsultLine",
          "name": "Consult Line",
          "when": "clearfeld.consultLine"
        }
      ]
    },
    "commands": [
      {
        "command": "clearfeld.consultLine",
        "category": "clearfeld",
        "title": "Consult Line"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
